     "description": {
      "ru": "Укажи хотя бы одну социальную сеть."
     },
     "isRequired": true,
     "validators": [
      {
       "type": "expression",
       "expression": "{chat.tg} notempty or {chat.vk} notempty"
      }
     ],
     "items": [
      {
       "name": "vk",
       "placeholder": {
        "ru": "@me"
       },
       "title": {
        "ru": "ВКонтакте"
       },
       "validators": [
        {
         "type": "regex",
         "text": {
          "ru": "Укажи ник в формате «@nick»."
         },
         "regex": "@.+"
        }
       ]
      },
      {
       "name": "tg",
       "placeholder": {
        "ru": "@also_me"
       },
       "title": {
        "ru": "Telegram"
       },
       "validators": [
        {
         "type": "regex",
         "text": {
          "ru": "Укажи ник в формате «@nick»."
         },
         "regex": "@.+"
        }
       ]
      }
     ]
    },
    {
     "type": "text",
