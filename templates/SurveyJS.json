                                {
                                    "type": "regex",
                                    "text": {
                                        "ru": "Укажи ник в формате «@nick»."
                                    },
                                    "regex": "@.+"
                                }
                            ]
                        },
                        {
                            "name": "tg",
                            "placeholder": {
                                "ru": "@also_me"
                            },
                            "title": {
                                "ru": "Telegram"
                            },
                            "validators": [
                                {
                                    "type": "regex",
                                    "text": {
                                        "ru": "Укажи ник в формате «@nick»."
                                    },
                                    "regex": "@.+"
                                }
                            ]
                        }
                    ]
